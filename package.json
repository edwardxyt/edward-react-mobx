{
  "name": "edward-react-mobx",
  "version": "1.0.0",
  "description": "react & mobx",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">= 8"
  },
  "babel": {
    "presets": ["env", "react", "stage-2"],
    "plugins": [
      "transform-decorators-legacy",
      ["import", { "libraryName": "antd", "style": "css" }]
    ]
  },
  "config": {
    "port": "7001",
    "express_port": "8001"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "scripts": {
    "clean": "rimraf node_modules",
    "dev": "DEBUG=development:* NODE_ENV='development' webpack-dev-server",
    "precompile": "DEBUG=compile:* node bin/del",
    "compile": "DEBUG=compile:* node bin/compile",
    "node:server": "DEBUG=test:* NODE_ENV='test' nodemon bin/node.server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwardxyt/edward-react-mobx.git"
  },
  "keywords": ["react", "mobx"],
  "author": "xiayuting <56833517@qq.com> (https://github.com/edwardxyt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edwardxyt/edward-react-mobx/issues"
  },
  "homepage": "https://github.com/edwardxyt/edward-react-mobx#readme",
  "devDependencies": {
    "antd": "^3.3.3",
    "autoprefixer": "^8.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.9",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^3.1.0",
    "ip": "^1.1.5",
    "lodash": "^4.17.5",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "nodemon": "^1.17.2",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "vconsole": "^3.1.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {}
}
